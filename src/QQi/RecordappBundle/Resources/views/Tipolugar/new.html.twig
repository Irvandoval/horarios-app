{% extends 'QQiRecordappBundle::layout.html.twig' %}

{% block title %}Nuevo Tipolugar {% endblock %}

{% block body %}
<div class="container">
  <form action="{{ path('tipolugar_create') }}" method="post" {{ form_enctype(form) }}>
    {{ form_widget(form) }}
    <p>
        <input class="btn btn-success btn-sm" type="submit" value="Crear">
    </p>
</form>

<div class="row">
        <a class="btn btn-primary btn-sm" href="{{ path('tipolugar') }}">
            Regresar a listado
        </a>
</div>
</div>

{% endblock %}
