{% extends 'QQiRecordappBundle::layout.html.twig' %}

{% block title %}Creacion de Ciclo {% endblock %}

{% block body %}

<div class="container">

<form action="{{ path('ciclo_create') }}" method="post" {{ form_enctype(form) }}>
    {{ form_widget(form) }}
<input class="btn btn-success btn-sm" type="submit" value="Crear">
</form>

<div class="row">
    <a class="btn btn-primary btn-sm" href="{{ path('ciclo') }}">
        Regresar a listado
    </a>
</div>
</div>

{% endblock %}
